		"react-native-svg-transformer": "~1.5.1",
		"react-test-renderer": "~19.0.0",
		"typescript": "~5.8.3"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": [
			"node remove_console.js",
			"prettier --write",
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "yarn@4.9.2"
}
